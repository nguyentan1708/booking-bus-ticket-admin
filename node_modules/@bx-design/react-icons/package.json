{
  "name": "@bx-design/react-icons",
  "version": "0.3.4",
  "repository": "git@github.com:bx-design/react-icons.git",
  "author": "BX Creative Studio",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "react-native": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rimraf ./dist && yarn build:esm",
    "build:esm": "bunchee src/index.tsx -m --no-sourcemap",
    "get-icons": "node ./scripts/get-icons.js && node ./scripts/icons-to-index.js && yarn format:fix",
    "types:check": "tsc --noEmit",
    "format": "prettier --check --debug-check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn types:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:fix",
      "yarn format:fix"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "bunchee": "^1.7.1",
    "camelcase": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "svg-parser": "^2.0.4",
    "svgo": "^2.6.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": "^16.11.0 || ^17.0.0"
  },
  "dependencies": {}
}
